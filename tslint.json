{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "no-var-requires": false,
        "indent": [
            true, 
            "spaces", 
            4
        ],
        "linebreak-style": [
            true, 
            "LF"
        ],
        "interface-name": [
            false, 
            "never-prefix"
        ]
    },
    "rulesDirectory": []
}
